#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/node/.cache/pnpm/dlx/29316a62d20ef5112b0005b3098b22037dd9ea3e7cc9c3cc8ec1f496f4d7486e/1976b9d90e8-c99/node_modules/.pnpm/create-astro@4.12.1/node_modules/create-astro/node_modules:/home/node/.cache/pnpm/dlx/29316a62d20ef5112b0005b3098b22037dd9ea3e7cc9c3cc8ec1f496f4d7486e/1976b9d90e8-c99/node_modules/.pnpm/create-astro@4.12.1/node_modules:/home/node/.cache/pnpm/dlx/29316a62d20ef5112b0005b3098b22037dd9ea3e7cc9c3cc8ec1f496f4d7486e/1976b9d90e8-c99/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/node/.cache/pnpm/dlx/29316a62d20ef5112b0005b3098b22037dd9ea3e7cc9c3cc8ec1f496f4d7486e/1976b9d90e8-c99/node_modules/.pnpm/create-astro@4.12.1/node_modules/create-astro/node_modules:/home/node/.cache/pnpm/dlx/29316a62d20ef5112b0005b3098b22037dd9ea3e7cc9c3cc8ec1f496f4d7486e/1976b9d90e8-c99/node_modules/.pnpm/create-astro@4.12.1/node_modules:/home/node/.cache/pnpm/dlx/29316a62d20ef5112b0005b3098b22037dd9ea3e7cc9c3cc8ec1f496f4d7486e/1976b9d90e8-c99/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/create-astro@4.12.1/node_modules/create-astro/create-astro.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/create-astro@4.12.1/node_modules/create-astro/create-astro.mjs" "$@"
fi
